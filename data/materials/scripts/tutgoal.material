
material tutgoal/translucent
{
	receive_shadows off

	technique
	{
		pass
		{
			ambient 0 0 0 0.1
			emissive 1 0 0 0.1
			scene_blend add
		}

		pass
		{
			emissive 1 1 1 1
			polygon_mode points
		}

	}

}

material tutgoal/translucent_green
{
	receive_shadows off

	technique
	{
		pass
		{
			ambient 0 0 0 0.1
			emissive 0 1 0 0.1
			scene_blend add
		}
		
		pass
		{
			emissive 1 1 1 1
			polygon_mode wireframe
		}


	}

}

