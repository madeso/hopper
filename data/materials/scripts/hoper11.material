
material hoper11/cockpit
{
	technique
	{
		pass
		{
			ambient 1 1 1 1
			// diffuse 1 1 1 1
			specular 1.0 1.0 1.0 127

			texture_unit
			{
				texture_alias 0
				texture metals.png
			}
		}

	}

}

